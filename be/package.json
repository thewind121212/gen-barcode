{
  "name": "express-api-starter-ts",
  "version": "0.0.0",
  "description": "A basic starter for an express.js v5 API with Typescript",
  "author": "CJ R. <cj@null.computer> (https://w3cj.now.sh)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/express-api-starter-ts.git"
  },
  "keywords": [],
  "main": "core/index.ts",
  "scripts": {
    "preview": "bunx prisma generate && bun --env-file=.env dist/core/index.js",
    "dev": "bunx tsx watch --env-file=.env core/index.ts",
    "build": "bunx prisma generate && bunx tsc",
    "lint": "bunx eslint --fix core test",
    "test": "bunx vitest run",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.2.0",
    "@sentry/node": "^8.0.0",
    "bunx": "^0.1.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "supertokens-node": "^23.1.0",
    "supertokens-web-js": "^0.16.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/morgan": "^1.9.10",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "eslint-plugin-format": "^1.0.2",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "vitest": "^4.0.6"
  }
}
